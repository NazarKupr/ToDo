  "description": "Todo app with Angular, AngularFire2, and Firebase",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.1",
    "npm": ">= 5"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 10"
  ],
  "scripts": {
